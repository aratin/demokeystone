extends ../layouts/default
block content
    header
      include ../views/navbar
      include ../views/menu
     section
      #inner-banner
      #inner-content-box#content-box
        .container
          #offerings
            .col-md-9.text-right
              ul.list-inline
                li
                  a(href="#",onclick="printFunction()")
                    i.fa.fa-lg.fa-print
                li
                  |
                li
                  a#pdf
                    i.fa.fa-lg.fa-file-pdf-o
                li
                  |
                li
                  a(href="/message")
                    i.fa.fa-lg.fa-envelope
            .col-md-9
              .row
                .col-md-3
                  img.img-responsive.img-thumbnail(src='#{data.partners[0].partnerimage}')
                .col-md-9
                  p
                    | #{data.partners[0].partnertext}
                    br
                    br
              .row
                .col-md-3
                  img.img-responsive.img-thumbnail(src='#{data.partners[0].partnersubimage}')
                .col-md-9
                  p
                    | #{data.partners[0].partnersubtext}
            include ../views/companyinfolistmenu
      .container-fluid
        .row
          footer
            include ../views/footer
            