extends ../layouts/default
block content
    header
      include ../views/navbar
      include ../views/menu
     section
      #inner-banner
      #inner-content-box#content-box
        .container
          #offerings
            .col-md-9.text-right
              ul.list-inline
                li
                  a#print
                    i.fa.fa-lg.fa-print
                li
                  |
                li
                  a#pdf
                    i.fa.fa-lg.fa-file-pdf-o
                li
                  |
                li
                  a(href="/message")
                    i.fa.fa-lg.fa-envelope
            .col-md-9
              .row
                .col-md-4
                  if data.consultingservices.length
                    .msp-images
                      if data.consultingservices[0].consultingServiceimage.exists
                        .row
                          .col-xs-12
                            a(href=data.consultingservices[0]._.consultingServiceimage.limit(900,600),rel='Consultingservice').msp-image
                              img(src=data.consultingservices[0]._.consultingServiceimage.limit(640,640))
                .col-md-8
                  p
                    | #{data.consultingservices[0].consultingServicetext}
            include ../views/listingmenu
      .container-fluid
        .row
          footer
            include ../views/footer
   