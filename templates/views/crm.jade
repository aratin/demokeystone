extends ../layouts/default
block content
    header
      include ../views/navbar
      include ../views/menu
     section
      #inner-banner
      #inner-content-box#content-box
        .container
          #offerings
            .col-md-9.text-right
              ul.list-inline
                li
                  a(href="#",onclick="printFunction()")
                    i.fa.fa-lg.fa-print
                li
                li
                  a#pdf
                    i.fa.fa-lg.fa-file-pdf-o
                li
                li
                  a(href="/message")
                    i.fa.fa-lg.fa-envelope
            .col-md-9
              .row
                .col-md-4
                  if data.crms.length
                        img(id="image",src=data.crms[0]._.image.limit(640,640))
                .col-md-8
                    p
                      | #{data.crms[0].html}
            include ../views/listingmenu
      .container-fluid
        .row
          footer
            include ../views/footer